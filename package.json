  },
  "dependencies": {
    "formik": "^2.4.5",
    "next": "14.0.4",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "react": "^18",
    "react-dom": "^18",
    "react-fast-marquee": "^1.6.2",
    "react-icons": "^4.12.0",
    "react-phone-number-input": "^3.3.7",
    "react-scroll": "^1.9.0",
    "react-slick": "^0.29.0",
    "slick-carousel": "^1.8.1",
    "swiper": "^11.0.5",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.8.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "eslint-plugin-next-on-pages": "^1.8.2",
